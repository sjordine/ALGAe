package jbio.alignment;

/**
 *
 * @author sergio
 */
public class Matrix {

    private String name;
    private int[][] scores;

    /**
     * @return the name
     */
    public String getName() {
        return name;
    }

    /**
     * @param name the name to set
     */
    public void setName(String name) {
        this.name = name;
    }

    /**
     * @return the scores
     */
    public int[][] getScores() {
        return scores;
    }

    /**
     * @param scores the scores to set
     */
    public void setScores(int[][] scores) {
        this.scores = scores;
    }




}
