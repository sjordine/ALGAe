package craligner.conservedregion;

/**
 *
 * @author sergio
 */
public class SubAlignment {

    private int startSequence1;
    private int startSequence2;

    private int endSequence1;
    private int endSequence2;

    private String alignedSequence1;
    private String alignedSequence2;

    private double score;

    /**
     * @return the startSequence1
     */
    public int getStartSequence1() {
        return startSequence1;
    }

    /**
     * @param startSequence1 the startSequence1 to set
     */
    public void setStartSequence1(int startSequence1) {
        this.startSequence1 = startSequence1;
    }

    /**
     * @return the startSequence2
     */
    public int getStartSequence2() {
        return startSequence2;
    }

    /**
     * @param startSequence2 the startSequence2 to set
     */
    public void setStartSequence2(int startSequence2) {
        this.startSequence2 = startSequence2;
    }

    /**
     * @return the endSequence1
     */
    public int getEndSequence1() {
        return endSequence1;
    }

    /**
     * @param endSequence1 the endSequence1 to set
     */
    public void setEndSequence1(int endSequence1) {
        this.endSequence1 = endSequence1;
    }

    /**
     * @return the endSequence2
     */
    public int getEndSequence2() {
        return endSequence2;
    }

    /**
     * @param endSequence2 the endSequence2 to set
     */
    public void setEndSequence2(int endSequence2) {
        this.endSequence2 = endSequence2;
    }

    /**
     * @return the alignedSequence1
     */
    public String getAlignedSequence1() {
        return alignedSequence1;
    }

    /**
     * @param alignedSequence1 the alignedSequence1 to set
     */
    public void setAlignedSequence1(String alignedSequence1) {
        this.alignedSequence1 = alignedSequence1;
    }

    /**
     * @return the alignedSequence2
     */
    public String getAlignedSequence2() {
        return alignedSequence2;
    }

    /**
     * @param alignedSequence2 the alignedSequence2 to set
     */
    public void setAlignedSequence2(String alignedSequence2) {
        this.alignedSequence2 = alignedSequence2;
    }


    /**
     * @return the score
     */
    public double getScore() {
        return score;
    }

    /**
     * @param score the score to set
     */
    public void setScore(double score) {
        this.score = score;
    }

}
